                            <!--<%= link_to "Tickets", tickets_path %>-->


                            <% if user_signed_in? %>
                            <%= link_to('Logout', destroy_user_session_path, method: :delete ) %>
                            <% else %>
                            <!--<%= link_to('Login', new_user_session_path ) %>-->
                            <% end %>


                        </div>
                    </div>
                </div>

                <div class="col-8 col-lg-12 brand-col"><a class="navbar-brand" href="#"><%= image_tag("logo-primary.png", :class => "navbar-logo") %></a></div>

                <div class="col-2 buy-ticket-col" id="floating_button"><button onclick="window.location.href = '/tickets';" class="floating-spooky button-glow">
                    <span class="noth2">Buy Tickets</span>
                    </button></div>

            </div>




        </nav>

 
        <% if notice %>
        <p class="notice"><%= notice %></p>
        <% end %>
        <% if alert %>
        <p class="alert"><%= alert %></p>
        <% end %>
        <%= yield %>

