                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>


                    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                        <div class="navbar-nav mr-auto">
                            <a onclick="window.location.href = '/';" class="nav-tickets">Home</a>
                            <a onclick="window.location.href = '/attractions';" class="nav-tickets">Attractions</a>
                            <a onclick="window.location.href = '/directions';" class="nav-tickets">Directions</a>
                            <a onclick="window.location.href = '/photos';" class="nav-tickets">Photos</a>
                            <a onclick="window.location.href = '/faq';" class="nav-tickets">FAQ</a>
                            
                            <!--<%= link_to "Home", root_path %>
